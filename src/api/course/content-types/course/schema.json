{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "teacher": {
      "type": "string"
    },
    "teacher_image": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "description": {
      "type": "text"
    },
    "big_description": {
      "type": "text"
    },
    "price": {
      "type": "string"
    },
    "created": {
      "type": "datetime"
    },
    "videos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::video.video",
      "mappedBy": "course"
    },
    "pdfs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::pdf.pdf",
      "mappedBy": "course"
    },
    "form_users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::form-user.form-user",
      "inversedBy": "courses"
    }
  }
}
